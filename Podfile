# platform :ios, '9.0'

target 'MVVM-SNS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MVVM-SNS
  pod 'RxSwift', '~>4.0.0'
  pod 'RxCocoa', '~>4.0.0'

  pod 'Swinject'
  pod 'SwinjectStoryboard'

  target 'MVVM-SNSTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick', '~> 1.3.0'
    pod 'Nimble', '~> 6.0.0'
  end

end
